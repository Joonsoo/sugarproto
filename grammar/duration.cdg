Duration = (Days WS)? (Hours WS)? (Minutes WS)? Seconds {Duration(days=$0, hours=$1, minutes=$2, seconds=$3}
  | (Days WS)? (Hours WS)? Minutes {Duration($0, $1, $2, null)}
  | (Days WS)? Hours {Duration($0, $1, null, null)}
  | Days {Duration($0, null, null, null)}
Days = Number WS 'd' $0
Hours = Number WS 'h' $0
Minutes = Number WS 'm' $0
Seconds = Number ('.' Number)? WS 's' {Seconds(integral=$0, fractional=$1)}
Number = '0' {"0"} | '1-9' '0-9'* {str($0, $1)}
WS = ' '*
